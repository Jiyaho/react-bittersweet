f (date < 10) {\n      return \"0\" + date;\n    } else {\n      return date;\n    }\n  };\n\n  const [noticeContent, setNoticeContent] = useState({\n    idx: \"\",\n    title: \"\",\n    content: \"\",\n    date: `${year}-${getMonth()}-${getDate()}`,\n  });\n\n  const getValue = (e) => {\n    const { name, value } = e.target;\n    setNoticeContent({\n      ...noticeContent,\n      [name]: value,\n    });\n    console.log(noticeContent);\n  };\n\n  const onSubmit = (e) => {\n    e.preventDefault();\n  };\n\n  return (\n    <div>\n      <Nav />\n      <h2 className={styles.page_title}>WRITE</h2>\n      <div className={styles.wrapNoticeWrite}>\n        <div>\n          <form onSubmit={onSubmit}>\n            <br />\n            <input\n              placeholder=\"제목을 입력해 주세요.\"\n              type=\"text\"\n              required=\"required\"\n              max=\"10\"\n              className={styles.titleInput}\n              onChange={getValue}\n              name=\"title\"\n            ></input>\n            <div className={styles.contentWrap}>\n              <textarea\n                cols=\"50\"\n                rows=\"10\"\n                placeholder=\"내용을 입력해 주세요.\"\n                onChange={getValue}\n                name=\"content\"\n              ></textarea>\n            </div>\n            <button\n              type=\"submit\"\n              required=\"required\"\n              className={styles.postBtn}\n              onSubmit={onSubmit}\n            >\n              등록\n            </button>\n            {/* <input\n                type=\"submit\"\n                required=\"required\"\n                className={styles.contentInput}\n                onSubmit={onSubmit}\n              ></input> */}\n          </form>\n        </div>\n      </div>\n      <Footer />\n    </div>\n  );\n}\nexport default NoticeWrite;\n","/Users/jiho/Desktop/dev/project/react-bittersweet/client/src/routes/Faq.js",["149"],[],"import { useEffect, useState } from \"react\";\n